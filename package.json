{
  "name": "dwc-website",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -C apps/web build",
    "start": "pnpm -C apps/web start",
    "lint": "pnpm -C apps/web lint",
    "typecheck": "pnpm -C apps/web typecheck",
    "db:generate": "pnpm -C apps/web prisma generate",
    "db:migrate": "pnpm -C apps/web prisma migrate dev",
    "db:studio": "pnpm -C apps/web prisma studio"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.41.1"
    }
  }
}

